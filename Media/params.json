{
  "datasetGroup": {
    "serviceConfig": {
      "name": "personalize-immersion-day",
      "domain": "VIDEO_ON_DEMAND"
    }
  },
  "datasets": {
    "interactions": {
      "dataset": {
        "serviceConfig": {
          "name": "workshop_movielens_interactions"
        }
      },
      "schema": {
        "serviceConfig": {
          "name": "workshop_movielens_interactions_schema",
          "domain": "VIDEO_ON_DEMAND",
          "schema": {
            "type": "record",
            "name": "interactions",
            "namespace": "com.amazonaws.personalize.schema",
            "fields": [
              {
                "name": "USER_ID",
                "type": "string"
              },
              {
                "name": "ITEM_ID",
                "type": "string"
              },
              {
                "name": "EVENT_TYPE",
                "type": "string"
              },
              {
                "name": "TIMESTAMP",
                "type": "long"
              }
            ]
          }
        }
      }
    },
    "users": {
      "dataset": {
        "serviceConfig": {
          "name": "workshop_movielens_users"
        }
      },
      "schema": {
        "serviceConfig": {
          "name": "workshop_movielens_users_schema",
          "domain": "VIDEO_ON_DEMAND",
          "schema": {
            "type": "record",
            "name": "users",
            "namespace": "com.amazonaws.personalize.schema",
            "fields": [
              {
                "name": "USER_ID",
                "type": "string"
              },
              {
                "name": "MEMBERLEVEL",
                "type": "string",
                "categorical": true
              }
            ]
          }
        }
      }
    },
    "items": {
      "dataset": {
        "serviceConfig": {
          "name": "workshop_movielens_items"
        }
      },
      "schema": {
        "serviceConfig": {
          "name": "workshop_movielens_items_schema",
          "domain": "VIDEO_ON_DEMAND",
          "schema": {
            "type": "record",
            "name": "items",
            "namespace": "com.amazonaws.personalize.schema",
            "fields": [
              {
                "name": "ITEM_ID",
                "type": "string"
              },
              {
                "name": "TITLE",
                "type": "string"
              },
              {
                "name": "YEAR",
                "type": "int"
              },
              {
                "name": "IMDB_RATING",
                "type": "int"
              },
              {
                "name": "IMDB_NUMBEROFVOTES",
                "type": "int"
              },
              {
                "name": "PLOT",
                "type": "string",
                "textual": true
              },
              {
                "name": "US_MATURITY_RATING_STRING",
                "type": "string"
              },
              {
                "name": "US_MATURITY_RATING",
                "type": "int"
              },
              {
                "name": "GENRES",
                "type": "string",
                "categorical": true
              },
              {
                "name": "CREATION_TIMESTAMP",
                "type": "long"
              },
              {
                "name": "PROMOTION",
                "type": "string"
              }
            ]
          }
        }
      }
    }
  },
  "recommenders": [
    {
      "serviceConfig": {
        "name": "workshop_more_like_x",
        "recipeArn": "arn:aws:personalize:::recipe/aws-vod-more-like-x"
      }
    },
    {
      "serviceConfig": {
        "name": "workshop_top_picks_for_you",
